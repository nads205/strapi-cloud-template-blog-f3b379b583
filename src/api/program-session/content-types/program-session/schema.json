{
  "kind": "collectionType",
  "collectionName": "program_sessions",
  "info": {
    "singularName": "program-session",
    "pluralName": "program-sessions",
    "displayName": "Program Session"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "session_name": {
      "type": "string",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date",
      "required": true
    },
    "capacity": {
      "type": "integer",
      "default": 20
    },
    "current_applications": {
      "type": "integer",
      "default": 0
    },
    "session_status": {
      "type": "enumeration",
      "enum": [
        "Open",
        "Full",
        "Closed",
        "Completed"
      ]
    },
    "description": {
      "type": "blocks"
    }
  }
}
