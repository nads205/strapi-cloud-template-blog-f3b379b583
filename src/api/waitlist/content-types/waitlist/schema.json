{
  "kind": "collectionType",
  "collectionName": "waitlists",
  "info": {
    "singularName": "waitlist",
    "pluralName": "waitlists",
    "displayName": "Waitlist"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "year_level": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Year 10",
        "Year 11",
        "Year 12",
        "Year 13",
        "University 1st Year",
        "University 2nd Year",
        "University 3rd Year",
        "Other"
      ]
    },
    "school_or_university": {
      "type": "string"
    },
    "interest_level": {
      "type": "enumeration",
      "required": true,
      "default": "Summer 2026",
      "enum": [
        "Summer 2025",
        "Autumn 2025",
        "Spring 2026",
        "Summer 2026",
        "Any time"
      ]
    },
    "additional_comments": {
      "type": "text"
    },
    "status": {
      "type": "string"
    },
    "submitted_at": {
      "type": "datetime",
      "required": true
    }
  }
}
